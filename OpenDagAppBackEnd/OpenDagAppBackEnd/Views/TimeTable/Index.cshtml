@model IEnumerable<OpenDagAppBackEnd.Models.TimeTable>

@{
    ViewBag.Title = "Index";
}

<div class="bodyForms">
    <h2>Index</h2>

    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                <b>Datum</b>
                @* @Html.DisplayNameFor(model => model.Date)*@
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @*@Html.DisplayFor(modelItem => item.Date.Date.ToLongDateString())*@
                    @Html.DisplayFor(modelItem => item.Date.Date.Day) -
                    @Html.DisplayFor(modelItem => item.Date.Date.Month) -
                    @Html.DisplayFor(modelItem => item.Date.Date.Year)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                </td>
            </tr>
        }

    </table>
</div>